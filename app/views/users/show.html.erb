
<div class="profileImgDiv">
 <img class="userImg" src="<%= @user.propic %>" alt="">
 <h1 class="userName"><%= @user.name %></h1>
</div>
<!-- <h2><%= @user.location %></h2>
 -->
<!-- underneath will be user's reviews -->
<div id="revSection">
  <% @user.reviews.first(9).each do |review| %>
    <div class="homeRw">
      <tr>
      	<td><%= review.reviewable.name %></td>
        <br>
        <td>"<%= review.text %></td>""
         <td><%= review.stars %> &#9733;</td>
      </tr>
  </div>
    <% end %>
</div>